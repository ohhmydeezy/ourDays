{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APPWRITE_PROJECT_ID": "@EXPO_PUBLIC_APPWRITE_PROJECT_ID",
        "EXPO_PUBLIC_APPWRITE_ENDPOINT": "@EXPO_PUBLIC_APPWRITE_ENDPOINT",
        "EXPO_PUBLIC_APPWRITE_PROJECT_NAME": "@EXPO_PUBLIC_APPWRITE_PROJECT_NAME",
        "EXPO_PUBLIC_DB_ID": "@EXPO_PUBLIC_DB_ID",
        "EXPO_PUBLIC_EVENTS_TABLE_ID": "@EXPO_PUBLIC_EVENTS_TABLE_ID",
        "EXPO_PUBLIC_USER_COLLECTION_ID": "@EXPO_PUBLIC_USER_COLLECTION_ID",
        "EXPO_PUBLIC_GEMINI_API_KEY": "@EXPO_PUBLIC_GEMINI_API_KEY",
        "EXPO_PUBLIC_NATIVE_NOTIFY_APP_ID": "@EXPO_PUBLIC_NATIVE_NOTIFY_APP_ID",
        "EXPO_PUBLIC_NATIVE_NOTIFY_APP_TOKEN": "@EXPO_PUBLIC_NATIVE_NOTIFY_APP_TOKEN"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
